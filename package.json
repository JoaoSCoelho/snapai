{
  "name": "snapai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint",
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:electron\"",
    "dev:next": "next dev",
    "build:electron": "tsc -p tsconfig.electron.json && tsc-alias -p tsconfig.electron.json",
    "start:electron": "cross-env NODE_ENV=development electron dist/electron/main.js",
    "dev:electron": "wait-on http://localhost:3000 && npm run build:electron && npm run start:electron",
    "build": "next build && tsc -p tsconfig.electron.json && tsc-alias -p tsconfig.electron.json",
    "start": "cross-env NODE_ENV=production electron dist/electron/main.js"
  },
  "dependencies": {
    "@datastructures-js/priority-queue": "^6.3.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
