    "@hookform/resolvers": "^5.2.2",
    "@microlink/react-json-view": "^1.27.0",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@prisma/client": "^6.18.0",
    "@react-sigma/core": "^5.0.4",
    "@sigma/export-image": "^3.0.0",
    "axios": "^1.13.1",
    "dotenv": "^17.2.3",
    "electron": "^39.0.0",
    "graphology": "^0.26.0",
    "js-sdsl": "^4.4.2",
    "next": "^16.0.7",
    "nuqs": "^2.7.2",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.65.0",
    "sigma": "^3.0.2",
    "sonner": "^2.0.7",
    "swr": "^2.3.6",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4",
    "@types/electron": "^1.4.38",
    "@types/node": "^20.19.24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
